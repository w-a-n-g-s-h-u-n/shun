        <artifactId>shun-starter-sso</artifactId>
        <version>${revision}</version>
      </dependency>
      <dependency>
        <groupId>org.wangshun.shun</groupId>
        <artifactId>shun-starter-sso-webflux</artifactId>
        <version>${revision}</version>
      </dependency>
    </dependencies>
