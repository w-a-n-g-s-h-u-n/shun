  <modelVersion>4.0.0</modelVersion>
  <parent>
    <groupId>org.springframework.cloud</groupId>
    <artifactId>spring-cloud-dependencies</artifactId>
    <version>Hoxton.SR3</version>
    <relativePath></relativePath>
  </parent>
  <groupId>org.wangshun.shun</groupId>
  <artifactId>shun-dependencies</artifactId>
  <version>2.2.1</version>
  <packaging>pom</packaging>
  <name>shun-dependencies</name>
  <description>三方库版本管理</description>
  <url>https://projects.spring.io/spring-cloud/spring-cloud-dependencies/shun-dependencies/</url>
